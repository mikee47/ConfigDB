{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "uint8val": {
      "type": "integer",
      "minimum": 0,
      "maximum": 255
    },
    "int32val": {
      "type": "integer"
    },
    "intval": {
      "type": "integer",
      "minimum": -5,
      "maximum": 100,
      "default": -1
    },
    "int64val": {
      "type": "integer",
      "minimum": -1e11,
      "maximum": 1e11,
      "default": -1
    },
    "numval": {
      "type": "number",
      "minimum": -1,
      "maximum": 10,
      "default": 3.1415
    }
  }
}