{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "color": {
      "type": "string",
      "ctype": "Color",
      "enum": [
        "red",
        "green",
        "blue"
      ]
    },
    "simple-bool": {
      "type": "boolean"
    },
    "simple-string": {
      "type": "string",
      "alias": "basic-string",
      "@default": "SIMPLE_STRING"
    },
    "simple-int": {
      "type": "integer",
      "@minimum": "-5",
      "maximum": 100,
      "@default": "-1"
    },
    "simple-float": {
      "type": "number",
      "default": 3.141592654,
      "maximum": 1.2e8
    },
    "int_array": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 0,
        "maximum": 100
      },
      "@default": [
        "13",
        "0x1c",
        "32+7",
        40
      ]
    },
    "string_array": {
      "type": "array",
      "alias": [
        "basic_array",
        "old_array"
      ],
      "items": {
        "type": "string",
        "default": "abc"
      },
      "default": [
        "a",
        "b",
        "c"
      ]
    },
    "object_array": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "intval": {
            "type": "integer"
          },
          "stringval": {
            "type": "string"
          }
        }
      }
    },
    "array-store": {
      "type": "array",
      "store": true,
      "items": {
        "type": "string"
      }
    },
    "object-array-store": {
      "type": "array",
      "store": true,
      "items": {
        "type": "object",
        "properties": {
          "value": {
            "type": "integer"
          }
        }
      }
    }
  }
}