{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "color": {
      "type": "string",
      "ctype": "Color",
      "enum": [
        "red",
        "green",
        "blue"
      ]
    },
    "simple-bool": {
      "type": "boolean"
    },
    "simple-string": {
      "type": "string",
      "default": "donkey"
    },
    "simple-int": {
      "type": "integer",
      "minimum": -5,
      "maximum": 100,
      "default": -1
    },
    "simple-float": {
      "type": "number",
      "default": 3.141592654,
      "maximum": 1.2e8
    },
    "int_array": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 0,
        "maximum": 100
      },
      "default": [
        13,
        28,
        39,
        40
      ]
    },
    "string_array": {
      "type": "array",
      "items": {
        "type": "string",
        "default": "abc"
      },
      "default": [
        "a",
        "b",
        "c"
      ]
    },
    "object_array": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "intval": {
            "type": "integer"
          },
          "stringval": {
            "type": "string"
          }
        }
      }
    }
  }
}