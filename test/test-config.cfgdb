{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "simple-bool": {
      "type": "boolean"
    },
    "simple-string": {
      "type": "string",
      "default": "donkey"
    },
    "simple-int": {
      "type": "integer",
      "minimum": -5,
      "maximum": 100,
      "default": -1
    },
    "simple-float": {
      "type": "number",
      "default": 3.141592654
    },
    "int_array": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "string_array": {
      "type": "array",
      "items": {
        "type": "string",
        "default": "abc"
      }
    },
    "object_array": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "intval": {
            "type": "integer"
          },
          "stringval": {
            "type": "string"
          }
        }
      }
    }
  }
}