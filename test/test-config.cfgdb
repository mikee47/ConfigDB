{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "simple-bool": {
      "type": "boolean"
    },
    "simple-string": {
      "type": "string",
      "default": "donkey"
    },
    "int_array": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "string_array": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "object_array": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "intval": {
            "type": "integer"
          },
          "stringval": {
            "type": "string"
          }
        }
      }
    }
  }
}